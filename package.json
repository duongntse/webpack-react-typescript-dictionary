{
	"name": "Webpack_React_Typescript_Dictionary",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"check-types": "tsc",
		"build": "webpack --mode=production",
		"watch": "webpack --env.DEV --watch --mode=development",
		"start": "webpack-dev-server --config webpack.config.js --progress --colors --open --mode=development",
		"test": "jest",
		"test:watch": "npm test -- --watch",
		"debug": "node --inspect node_modules/webpack/bin/webpack.js --colors",
		"test:debug": "node --inspect node_modules/.bin/jest --watch --runInBand"
	},
	"jest": {
		"testEnvironment": "node",
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json"
		],
		"moduleDirectories": [
			"node_modules",
			"src"
		],
		"moduleNameMapper": {
			"^src/(.*)$": "<rootDir>/src/$1",
			"^root/(.*)$": "<rootDir>/$1",
			"^.+\\.(css|less|scss)$": "identity-obj-proxy"
		}
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
